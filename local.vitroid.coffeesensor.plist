<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>local.vitroid.coffeesensor</string>
	<key>StandardOutPath</key>
	<string>/var/tmp/local.vitroid.coffeesensor.log</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/bin/coffeesensor.sh</string>
	</array>
	<key>KeepAlive</key>
	<true/>
	<key>RunAtLoad</key>
	<true/>
</dict>
</plist>
